@model IEnumerable<BlackBoard.Models.Enroll>

@{
    ViewData["Title"] = "Index";
}

<h1>All student Enrollments</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Course.CourseName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Student.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Student.LastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Student.StudentAuId)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Course.CourseName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student.StudentAuId)
            </td>

            <td>
                <a asp-action="Edit" asp-route-id="@item.EnrollId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.EnrollId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.EnrollId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
